{
    "name": "polkastarter-js",
    "version": "2.2.7-nft-beta3",
    "description": "",
    "main": "dist/index.js",
    "engines": {
        "node": ">=10.13.0 <=^14"
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/polkastarter/polkastarter-js.git"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^3.2.0",
        "@walletconnect/web3-provider": "^1.7.1",
        "abi-decoder": "^1.2.0",
        "accounting": "^0.4.1",
        "axios": "^0.21.1",
        "big-number": "^2.0.0",
        "bignumber.js": "^9.0.1",
        "bn.js": "^5.1.3",
        "chai": "^4.2.0",
        "crypto-js": "^3.1.9-1",
        "dayjs": "^1.9.4",
        "decimal.js": "^10.2.1",
        "delay": "^4.4.0",
        "dotenv": "^8.2.0",
        "ethers": "^5.3.1",
        "execution-time": "^1.4.1",
        "express": "^4.17.1",
        "left-pad": "^1.3.0",
        "lodash": "^4.17.10",
        "mathjs": "^9.4.2",
        "mocha": "^5.1.1",
        "moment": "^2.21.0",
        "mongoose": "^5.9.5",
        "randomhex": "^0.1.5",
        "web3": "1.0.0-beta.55"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc -p .",
        "test": "nyc ./node_modules/.bin/mocha ./tests/index.ts --timeout 3000000 --require @babel/register --require @babel/polyfill --require ts-node/register --require source-map-support/register --recursive",
        "index": "babel-node ./index.js --presets es2015,stage-2",
        "docs:numbers": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/utils/Numbers.js > docs/NUMBERS.md",
        "docs:app": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/models/index.js > docs/APPLICATION.md",
        "docs:signer": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/utils/Signer.js > docs/SIGNER.md",
        "docs:swap": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/models/FixedSwapContract.js > docs/SWAP.md",
        "docs:nftswap": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/models/FixedNFTSwapContract.js > docs/NFT_SWAP.md",
        "docs:baseswap": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/models/BaseSwapContract.js > docs/BASE_SWAP.md",
        "docs:network": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/utils/Network.js > docs/NETWORK.md",
        "docs:staking": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/models/Staking.js > docs/STAKING.md",
        "docs:wallet": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/utils/Wallet.js > docs/WALLET.md",
        "docs:idostaking": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/models/IDOStaking.js > docs/IDOSTAKING.md",
        "docs": "npm run docs:numbers && npm run docs:signer && npm run docs:swap && npm run docs:network && npm run docs:app && npm run docs:staking && npm run docs:idostaking && npm run docs:wallet && npm run docs:nftswap && npm run docs:baseswap",
        "docs:prepare": "gitbook install",
        "docs:build": "npm run docs:prepare && npm run docs && gitbook build",
        "docs:serve": "npm run docs:prepare && npm run docs && gitbook serve",
        "docs:clean": "rimraf _book"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.16.4",
        "@babel/register": "^7.17.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/accounting": "^0.4.2",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "babel-plugin-transform-commonjs": "^1.1.6",
        "babel-register": "^6.26.0",
        "eslint": "^8.9.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-base-typescript": "^1.0.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-mocha": "^10.0.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "ganache-core": "^2.13.2",
        "gitbook-cli": "^2.3.2",
        "jsdoc-babel": "^0.5.0",
        "jsdoc-md": "^11.0.2",
        "jsdoc-to-markdown": "^7.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.5.1",
        "should": "^7.1.0",
        "source-map-support": "^0.5.21",
        "supertest": "^1.0.0",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.4"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ],
        "plugins": [
            "@babel/transform-runtime"
        ],
        "sourceMaps": true,
        "retainLines": true
    },
    "files": [
        "lib/**/*"
    ],
    "author": "",
    "license": "ISC"
}
